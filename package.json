{
  "name": "@sanity/webhook",
  "version": "3.0.1",
  "description": "Toolkit for dealing with GROQ-powered webhooks delivered by Sanity.io",
  "keywords": [
    "webhook",
    "webhooks",
    "sanity-io",
    "sanity",
    "verify",
    "validate"
  ],
  "homepage": "https://github.com/sanity-io/webhook-toolkit#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/webhook-toolkit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/webhook-toolkit.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "node": {
        "module": "./dist/index.mjs",
        "import": "./dist/index.cjs.mjs"
      },
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "pkg build --strict && pkg --strict",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    "last 1 chrome version",
    "maintained node versions"
  ],
  "eslintConfig": {
    "env": {
      "browser": false,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "sanity",
      "prettier"
    ],
    "rules": {
      "operator-linebreak": "off",
      "prettier/prettier": "error"
    },
    "root": true
  },
  "release": {
    "branches": [
      "main"
    ],
    "extends": "@sanity/semantic-release-preset"
  },
  "dependencies": {
    "base64url": "^3.0.1"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^2.4.6",
    "@sanity/semantic-release-preset": "^4.1.3",
    "@types/express": "^4.17.17",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "body-parser": "^1.20.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.18.2",
    "ls-engines": "^0.9.0",
    "prettier": "^3.0.2",
    "prettier-plugin-packagejson": "^2.4.5",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.7",
    "supertest": "^6.3.3",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
